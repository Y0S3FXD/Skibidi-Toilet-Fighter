@startuml

class SkibidiToilet {
  - health: float
  - stamina: float
  + Attack()
  + Dance()
  + Regenerate()
  + KnockOut()
}

class Item {
  - name: string
  - effect: enum
}

class Player {
  - playerName: string
  - score: int
  + Move()
  + UseItem()
  + ShowScore()
}

class GameController {
  + StartGame()
  + EndGame()
  + CalculateScore()
}

SkibidiToilet --|> Item
Player --|> GameController

@enduml
